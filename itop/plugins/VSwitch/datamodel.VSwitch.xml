          <neighbours>
            <neighbour id="lnkphysicalinterfacetovswitch" _delta="define">
              <query_down>SELECT VSwitch AS v JOIN lnkPhysicalInterfaceToVSwith AS o ON o.vswitch_id=v.id  WHERE o.physicalinterface_port = :this-&gt;id</query_down>
