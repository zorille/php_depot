    <class id="PhysicalInterface" _delta="must_exist">
      <presentation>
        <search>
          <items>
            <item id="connectableci_id">
              <rank>10</rank>
            </item>
          </items>
        </search>
      </presentation>
      <relations>
        <relation id="impacts">
          <neighbours>
            <neighbour id="lnkPhysicalInterfaceToVirtualSwitch" _delta="define">
              <query_down>SELECT VirtualSwitch AS v JOIN lnkPhysicalInterfaceToVirtualSwitch AS o ON o.virtualswitch_id=v.id  WHERE o.physicalinterface_port = :this-&gt;id</query_down>
              <query_up>SELECT ConnectableCI AS v JOIN PhysicalInterface AS p ON p.connectableci_id=v.id JOIN lnkPhysicalInterfaceToVirtualSwitch AS o ON o.physicalinterface_port=p.id WHERE o.virtualswitch_id = :this-&gt;id</query_up>
            </neighbour>
          </neighbours>
        </relation>
      </relations>
    </class>
