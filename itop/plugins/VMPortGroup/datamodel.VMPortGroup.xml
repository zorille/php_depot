    <class id="LogicalInterface" _delta="must_exist">
      <fields _delta="merge">
        <field id="vmportgroup_id" xsi:type="AttributeExternalKey" _delta="define">
          <sql>vmportgroup_id</sql>
          <target_class>VMPortGroup</target_class>
          <is_null_allowed>true</is_null_allowed>
          <on_target_delete>DEL_MANUAL</on_target_delete>
