            </item>
          </items>
        </details>
      </presentation>
      <relations>
        <relation id="impacts">
          <neighbours>
            <neighbour id="vmportgroups" _delta="define">
              <attribute>vmportgroups_list</attribute>
              <!-- <direction>down</direction>-->
