            <item id="macaddress">
              <rank>40</rank>
            </item>
            <item id="vlan_id">
              <rank>45</rank>
            </item>
            <item id="comment">
              <rank>50</rank>
            </item>
            <item id="speed">
              <rank>80</rank>
            </item>
          </items>
