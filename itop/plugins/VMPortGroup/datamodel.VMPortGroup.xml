      <methods/>
      <presentation>
        <details>
          <items>
			<item id="ip_list">
			  <rank>5</rank>
			</item>
			<item id="logicalinterfaces_list">
			  <rank>6</rank>
			</item>
			<item id="name">
              <rank>10</rank>
            </item>
            <item id="vswitch_id">
              <rank>20</rank>
