          <target_class>VLAN</target_class>
          <is_null_allowed>true</is_null_allowed>
          <on_target_delete>DEL_AUTO</on_target_delete>
        </field>
        <field id="vlan_name" xsi:type="AttributeExternalField" _delta="define">
