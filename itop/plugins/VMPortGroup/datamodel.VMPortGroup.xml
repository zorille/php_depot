        <reconciliation>
          <attributes>
            <attribute id="name"/>
            <attribute id="vswitch_id"/>
            <attribute id="vswitch_name"/>
          </attributes>
        </reconciliation>
      </properties>
      <fields>
        <field id="vswitch_id" xsi:type="AttributeExternalKey" _delta="define">
          <sql>vswitch_id</sql>
          <target_class>VSwitch</target_class>
          <is_null_allowed>false</is_null_allowed>
          <on_target_delete>DEL_AUTO</on_target_delete>
        </field>
        <field id="vswitch_name" xsi:type="AttributeExternalField" _delta="define">
          <extkey_attcode>vswitch_id</extkey_attcode>
          <target_attcode>name</target_attcode>
        </field>-->
