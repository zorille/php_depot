      </fields>
      <presentation>
        <details _delta="merge">
          <items>
            <item id="vmportgroup_id">
              <rank>80</rank>
            </item>
          </items>
        </details>
        <search _delta="merge">
          <items>
            <item id="vmportgroup_id">
              <rank>50</rank>
            </item>
          </items>
        </search>
        <list _delta="merge">
          <items>
            <item id="vmportgroup_id">
              <rank>70</rank>
            </item>
          </items>
