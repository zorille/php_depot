<?xml version="1.0" encoding="UTF-8"?>
<itop_design xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0">
  <constants>
  </constants>
  <classes>
    <class id="NetworkSocket" _delta="define">
      <parent>NetworkInterface</parent>
      <properties>
        <category>bizmodel,searchable</category>
        <abstract>false</abstract>
        <key_type>autoincrement</key_type>
        <db_table>networksocket</db_table>
        <db_key_field>id</db_key_field>
        <db_final_class_field/>
        <naming>
          <attributes>
            <attribute id="name"/>
            <!-- <attribute id="application_name"/>
            <attribute id="socketvalue"/>
	        <attribute id="networkinterface_name"/> -->
          </attributes>
        </naming>
        <display_template/>
        <icon>images/socket.png</icon>
        <reconciliation>
          <attributes>
            <attribute id="socketvalue"/>
            <attribute id="networkinterface_id"/>
            <attribute id="networkinterface_name"/>
            <attribute id="application_id"/>
            <attribute id="application_name"/>
          </attributes>
        </reconciliation>
      </properties>
      <fields>
      	<field id="socketvalue" xsi:type="AttributeString">
      	  <sql>socketvalue</sql>
          <default_value/>
          <is_null_allowed>false</is_null_allowed>
        </field>
        <field id="socketprotocol" xsi:type="AttributeEnum">
          <values>
            <value>tcp</value>
            <value>udp</value>
            <value>both</value>
          </values>
          <sql>socketprotocol</sql>
          <default_value/>
          <is_null_allowed>false</is_null_allowed>
          <display_style>radio_horizontal</display_style>
        </field>
        <field id="networkinterface_id" xsi:type="AttributeExternalKey">
          <filter><![CDATA[SELECT NetworkInterface WHERE finalclass IN ('physicalinterface','logicalinterface')]]></filter>
          <sql>networkinterface_id</sql>
          <target_class>NetworkInterface</target_class>
          <is_null_allowed>true</is_null_allowed>
          <on_target_delete>DEL_AUTO</on_target_delete>
          <allow_target_creation>false</allow_target_creation>
        </field>
        <field id="networkinterface_name" xsi:type="AttributeExternalField">
          <extkey_attcode>networkinterface_id</extkey_attcode>
          <target_attcode>name</target_attcode>
        </field>
        <!-- <field id="system_id" xsi:type="AttributeExternalKey">
          <filter><![CDATA[SELECT FunctionalCI WHERE finalclass IN ('Server','VirtualMachine','PC')]]></filter>
          <sql>system_id</sql>
          <target_class>FunctionalCI</target_class>
          <is_null_allowed>false</is_null_allowed>
          <on_target_delete>DEL_AUTO</on_target_delete>
          <allow_target_creation>false</allow_target_creation>
        </field>
        <field id="system_name" xsi:type="AttributeExternalField">
          <extkey_attcode>system_id</extkey_attcode>
          <target_attcode>name</target_attcode>
        </field> -->
        <field id="application_id" xsi:type="AttributeExternalKey">
          <filter><![CDATA[SELECT FunctionalCI WHERE finalclass IN ('Middleware','MiddlewareInstance','PCSoftware','OtherSoftware','WebServer','WebApplication','DBServer')]]></filter>
          <sql>application_id</sql>
          <target_class>FunctionalCI</target_class>
          <is_null_allowed>false</is_null_allowed>
          <on_target_delete>DEL_AUTO</on_target_delete>
          <allow_target_creation>false</allow_target_creation>
        </field>
        <field id="application_name" xsi:type="AttributeExternalField">
          <extkey_attcode>application_id</extkey_attcode>
          <target_attcode>name</target_attcode>
        </field>
      </fields>
      <methods/>
      <presentation>
        <details>
          <items>
            <item id="name">
              <rank>10</rank>
            </item>
	    	<item id="socketvalue">
              <rank>20</rank>
            </item>
            <item id="socketprotocol">
              <rank>30</rank>
            </item>
            <!-- <item id="system_id">
              <rank>40</rank>
            </item> -->
            <item id="application_id">
              <rank>50</rank>
            </item>
	   		<item id="networkinterface_id">
              <rank>60</rank>
            </item>
          </items>
        </details>
        <search>
          <items>
            <item id="name">
              <rank>10</rank>
            </item>
	    	<item id="socketvalue">
              <rank>20</rank>
            </item>
            <item id="socketprotocol">
              <rank>30</rank>
            </item>
            <!--  <item id="system_id">
              <rank>40</rank>
            </item> -->
            <item id="application_id">
              <rank>50</rank>
            </item>
          </items>
        </search>
        <list>
          <items>
            <item id="name">
              <rank>10</rank>
            </item>
	    	<item id="socketvalue">
              <rank>20</rank>
            </item>
            <item id="socketprotocol">
              <rank>30</rank>
            </item>
            <!-- <item id="system_id">
              <rank>40</rank>
            </item> -->
            <item id="application_id">
              <rank>50</rank>
            </item>
          </items>
        </list>
      </presentation>
    </class>
    <class id="NetworkInterface">
      <fields>
        <field id="networksocket_list" xsi:type="AttributeLinkedSet" _delta="define">
          <linked_class>NetworkSocket</linked_class>
          <ext_key_to_me>networkinterface_id</ext_key_to_me>
          <edit_mode>in_place</edit_mode>
          <tracking_level>all</tracking_level>
          <count_min>0</count_min>
          <count_max>0</count_max>
        </field>
      </fields>
      <relations>
        <relation id="impacts" _delta="define">
          <neighbours>
            <neighbour id="networksocket">
              <attribute>networksocket_list</attribute>
            </neighbour>
          </neighbours>
        </relation>
      </relations>
    </class>
    <class id="LogicalInterface">
      <presentation>
        <details _delta="merge">
          <items>
            <item id="networksocket_list">
              <rank>80</rank>
            </item>
          </items>
        </details>
      </presentation>
    </class>
    <class id="PhysicalInterface">
      <presentation>
        <details _delta="merge">
          <items>
            <item id="networksocket_list">
              <rank>100</rank>
            </item>
          </items>
        </details>
      </presentation>
    </class>
  </classes>
  <user_rights>
  </user_rights>
</itop_design>
