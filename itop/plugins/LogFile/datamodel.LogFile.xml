<?xml version="1.0" encoding="UTF-8"?>
<itop_design xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0">
  <constants>
  </constants>
  <classes>
    <class id="LogFile" _delta="define">
      <parent>FunctionalCI</parent>
      <properties>
        <category>bizmodel,searchable</category>
        <abstract>false</abstract>
        <key_type>autoincrement</key_type>
        <db_table>logfile</db_table>
        <db_key_field>id</db_key_field>
        <db_final_class_field/>
        <naming>
          <format>%1$s</format>
          <attributes>
            <attribute id="name"/>
            <attribute id="application_name"/>
          </attributes>
        </naming>
        <display_template/>
        <icon>images/LogFile.png</icon>
        <reconciliation>
          <attributes>
            <attribute id="name"/>
            <attribute id="org_id"/>
            <attribute id="organization_name"/>
            <attribute id="application_id"/>
          </attributes>
        </reconciliation>
      </properties>
      <fields>
        <field id="logName" xsi:type="AttributeString">
          <sql>logName</sql>
          <default_value/>
          <is_null_allowed>false</is_null_allowed>
        </field>
        <field id="logDirectory" xsi:type="AttributeString">
          <sql>logDirectory</sql>
          <default_value/>
          <is_null_allowed>false</is_null_allowed>
        </field>
        <field id="logRotate" xsi:type="AttributeEnum">
          <values>
            <value>Daily</value>
            <value>Size</value>
            <value>None</value>
          </values>
          <sql>logRotate</sql>
          <default_value>None</default_value>
          <is_null_allowed>true</is_null_allowed>
          <display_style>radio_horizontal</display_style>
        </field>
        <field id="application_id" xsi:type="AttributeExternalKey">
          <filter><![CDATA[SELECT FunctionalCI WHERE finalclass IN ('Middleware','MiddlewareInstance','PCSoftware','OtherSoftware','WebServer','WebApplication','DBServer')]]></filter>
          <sql>application_id</sql>
          <target_class>FunctionalCI</target_class>
          <is_null_allowed>false</is_null_allowed>
          <on_target_delete>DEL_AUTO</on_target_delete>
          <allow_target_creation>false</allow_target_creation>
        </field>
        <field id="application_name" xsi:type="AttributeExternalField">
          <extkey_attcode>application_id</extkey_attcode>
          <target_attcode>name</target_attcode>
        </field>
        <field id="logFormat" xsi:type="AttributeString">
          <sql>logFormat</sql>
          <default_value/>
          <is_null_allowed>true</is_null_allowed>
        </field>
      </fields>
      <methods/>
      <presentation>
        <details>
          <items>
            <item id="name">
              <rank>10</rank>
            </item>
            <item id="org_id">
              <rank>20</rank>
            </item>
            <item id="business_criticity">
              <rank>40</rank>
